{
    "eslint.enable": true,
    "eslint.run": "onSave",
    "eslint.packageManager": "npm",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "javascript.validate.enable": false,
    "typescript.validate.enable": false
  }
  